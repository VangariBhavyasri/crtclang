#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        
    }
    int sum1=0,sum2=0;
    for(i=0;i<n;i++)
        if(a[i]%2==0)
        {
            sum1=sum1+a[i];
        }
    else
    {
sum2=sum2+a[i];
    }
    printf("%d\n",sum2);
    printf("%d\n",sum1);
     return 0;
}
